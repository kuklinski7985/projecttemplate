void * motionSense(void* vars)
{
    printf("entering motionSense thread\n");
    while(bizzounce == 0);

    return 0;                   //return a void*
}

void * logger(void* vars)
{
    printf("entering logger thread\n");
    while(bizzounce == 0);

    return 0;
}